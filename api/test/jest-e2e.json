{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "moduleNameMapper": {
    "@http-message/(.*)": "<rootDir>/../../http-message/src/$1",
    "@decorater/(.*)": "<rootDir>/../src/decorater/$1"
  }
}
